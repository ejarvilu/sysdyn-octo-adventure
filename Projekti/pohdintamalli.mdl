Alkoholiin liittyvät terveysongelmat=
	MAX(DELAY3(Kokonaiskulutus,5), 0)
	~	
	~		|

Alkoholin kysyntä= ACTIVE INITIAL (
	SMOOTH(MAX(0,MIN(Fyysiset rajoitukset alkoholin kulutukselle,(2+Sosiaalinen hyväksyntä\
		+Alkoholoin arkipäiväistyminen) )),2),
		10)
	~	
	~		|

Alkoholoin arkipäiväistyminen=
	Kokonaiskulutus*1.5
	~	
	~		|

Anniskelu=
	0.1*Alkoholin tarve
	~	
	~		|

Fyysiset rajoitukset alkoholin kulutukselle=
	20
	~	
	~		|

Sosiaalinen hyväksyntä=
	-Alkoholiin liittyvät terveysongelmat
	~	
	~		|

Vähittäismyynti=
	1.2*Alkoholin tarve
	~	
	~		|

Alkoholin tarve= INTEG (
	Alkoholin kysyntä-Anniskelu-Vähittäismyynti-Maahantuonti,
		0)
	~	 [?,4]
	~		|

Kokonaiskulutus=
	MAX(Tilastoimaton kulutus+Tilastoitu kulutus,0)
	~	
	~		|

Maahantuonti=
	0.2*Alkoholin tarve
	~	
	~		|

Tilastoimaton kulutus=
	Maahantuonti
	~	
	~		|

Tilastoitu kulutus=
	Anniskelu+Vähittäismyynti
	~	
	~		|

